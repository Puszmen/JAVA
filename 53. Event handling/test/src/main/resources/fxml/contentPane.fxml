<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pl.javastart.test.controller.contentPaneController">
   <children>
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <TextArea fx:id="inputTextArea" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <TextArea fx:id="outputTextArea" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Button fx:id="clearButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Clear windows" VBox.vgrow="NEVER" />
   </children>
</VBox>
